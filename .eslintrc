{
  "rules": {
    "brace-style": [2, "stroustrup"],
    "comma-dangle": [2, "never"],
    "no-console": 2,
    "generator-star-spacing": [2, {"before": false, "after": true}],
    "strict": [2, "global"]
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": ["airbnb/base"],
  "parser": "babel-eslint"
}
